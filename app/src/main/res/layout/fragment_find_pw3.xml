<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".presentation.ui.auth.findinfo.screens.findPwFragment3">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="150dp"
        android:text="해당 이메일로 가입된 회원정보가 존재하지 않습니다"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.TeacherForBoss.Body.Medium.16"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <Button
        android:id="@+id/gotoSignupBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/radius_purple_button"
        android:text="회원가입하기"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_editor_absoluteX="0dp">

    </Button>

</androidx.constraintlayout.widget.ConstraintLayout>